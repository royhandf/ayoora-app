{
  "expo": {
    "name": "Ayoora App",
    "slug": "ayoora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ayooraapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.boedakoding.ayoora"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1C1C1E"
      },
      "package": "com.boedakoding.ayoora",
      "versionCode": 1,
      "usesCleartextTraffic": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1C1C1E"
        }
      ],
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "b9c106b1-ebdf-45ab-83b6-70c8dd81fa2f"
      }
    },
    "owner": "royhandf"
  }
}
